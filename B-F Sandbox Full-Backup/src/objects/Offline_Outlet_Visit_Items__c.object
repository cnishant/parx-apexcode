    <fields>
        <fullName>Data_Flag__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Available__c,1,
IF( Bottle_Price__c &gt; 0,1,
IF( Bottle_Price_Special__c , 1,
IF( Depletions_Last_Month__c &gt; 0, 1,
IF( Depletions_Next_Month__c &gt; 0, 1,
IF( Depletions_This_Month__c &gt; 0, 1,
IF( Double_Price__c &gt; 0, 1,
IF( Double_Price_Special__c,1,
IF( Drink_Price__c &gt; 0, 1,
IF( Drink_Price_Special__c,1,
IF( Facing__c &gt; 0, 1,
IF( NOT(ISPICKVAL(Shelf__c,&quot;&quot;)),1,0
))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Data Flag</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
