    <validationRules>
        <fullName>PreventChangeofSoxContactName</fullName>
        <active>true</active>
        <description>Prevents users from changing a custom field called Sox Contact (Name) after it has been saved initially - Used with Compensating Controls Application</description>
        <errorConditionFormula>AND( NOT( ISNEW() ), ISCHANGED(  Name ))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>SOX Contact cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventChangeofUniqueID</fullName>
        <active>true</active>
        <description>Prevents users from changing a custom field called UniqueID after it has been saved initially - Used with Compensating Controls Application</description>
        <errorConditionFormula>AND( NOT( ISNEW() ), ISCHANGED( UniqueID__c ))</errorConditionFormula>
        <errorDisplayField>UniqueID__c</errorDisplayField>
        <errorMessage>UniqueID cannot be changed</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>I_Certify_Complete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>I Certify Complete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}

var recId = &quot;{!SOD__c.Id}&quot;;
sforce.connection.sessionId = &quot;{!API.Session_ID}&quot;; 
//Call the class &apos;certificatioCheck&apos; with the method &apos;checkCertification&apos; 
var result = sforce.apex.execute(&quot;certificationCheck&quot;,&quot;checkCertification&quot;,{recordId:recId}); 

if(result==&apos;true&apos;)
{  
// Refresh window
window.location.reload(); 
}
if(result==&apos;false&apos;)
{
alert(&apos;Please select Compliance Method or Attach Proof of Controls !&apos;);
}</url>
