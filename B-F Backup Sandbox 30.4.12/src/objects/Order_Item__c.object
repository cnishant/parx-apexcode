    <validationRules>
        <fullName>Free_Stock_Discount_Check</fullName>
        <active>true</active>
        <description>Purpose: To ensure we don&apos;t have both Free Stock and Discount on the Same Item Level</description>
        <errorConditionFormula>AND((Carton_Bonus__c   &gt; 0  ||   Unit_Bonus__c  &gt; 0) &amp;&amp;  Discount__c &gt; 0)</errorConditionFormula>
        <errorMessage>Free Stock &amp; Discounts can NOT be applied to the same item level!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Item_Locked</fullName>
        <active>true</active>
        <description>If status is set to processed order then order items cannot be edited.</description>
        <errorConditionFormula>ISPICKVAL(Order__r.Status__c , &apos;Process Order&apos;)  || ISPICKVAL(Order__r.Status__c , &apos;Complete&apos;)</errorConditionFormula>
        <errorMessage>Order has already been processed. Changes cannot be made.  Please contact customer service.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Price_Tolerance_Check</fullName>
        <active>true</active>
        <description>Validation to see if Price Override will work</description>
        <errorConditionFormula>Discount__c   &gt;  Max_PA_Per_Case__c</errorConditionFormula>
        <errorDisplayField>Discount__c</errorDisplayField>
        <errorMessage>Price Tolerance Reached - Please Enter a Lower Amount</errorMessage>
    </validationRules>
