    <fields>
        <fullName>Purchased_From_f__c</fullName>
        <description>FDC-Art Collection</description>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Purchased_From__c,&quot;Artist&quot;) ,  Artist__r.First_Name__c &amp;&quot; &quot;&amp; Artist__r.Last_Name__c ,  Gallery__r.Name )</formula>
