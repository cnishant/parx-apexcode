    <validationRules>
        <fullName>Select_Gallery_if_Purchased_From_a_Galle</fullName>
        <active>false</active>
        <description>User must select a gallery if they enter that the art asset was purchased from a gallery.</description>
        <errorConditionFormula>AND( ISPICKVAL(Purchased_From__c,&quot;Gallery&quot;) , Gallery__c =NULL)</errorConditionFormula>
        <errorDisplayField>Gallery__c</errorDisplayField>
        <errorMessage>If you select the Purchased From value of &quot;Gallery&quot;, you must enter a Gallery.</errorMessage>
    </validationRules>
