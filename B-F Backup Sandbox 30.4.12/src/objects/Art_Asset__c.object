        <label>Purchased From (f)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Size_f__c</fullName>
        <description>FDC-Art Collection Database</description>
        <externalId>false</externalId>
        <formula>IF(Height_in__c=&quot;&quot;,&quot;&quot;,IF( Depth_in__c = &quot;&quot;,Height_in__c  &amp;&quot;&apos;&apos; x &quot;&amp; Width_in__c&amp;&quot;&apos;&apos;&quot;,Height_in__c&amp;&quot;&apos;&apos; x &quot;&amp; Width_in__c&amp;&quot;&apos;&apos; x &quot;&amp;  Depth_in__c&amp;&quot;&apos;&apos;&quot; ))</formula>
        <inlineHelpText>Calculates the size based on Width, Height and Depth entered.</inlineHelpText>
        <label>Size (f)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
