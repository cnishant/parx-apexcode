    <listViews>
        <fullName>Missing_Fixed_Asset_Number</fullName>
        <columns>NAME</columns>
        <columns>Art_Location__c</columns>
        <columns>Cost__c</columns>
        <columns>Fixed_Asset_Number__c</columns>
        <columns>Primary_Medium__c</columns>
        <columns>Medium_Description__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Fixed_Asset_Number__c</field>
