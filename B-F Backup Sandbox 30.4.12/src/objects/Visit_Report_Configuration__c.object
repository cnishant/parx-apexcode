        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var rid=&quot;{!Visit_Report_Configuration__c.Id}&quot;; 

var result = sforce.apex.execute( 
&quot;VisitReportGeneratorBatchable&quot;,&quot;collectVisitItems&quot;,{visitreportconfigurationid: rid}); 

alert(result);</url>
    </webLinks>
