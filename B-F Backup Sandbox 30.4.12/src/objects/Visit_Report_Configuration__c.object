    <validationRules>
        <fullName>RecurrenceFreeSelection</fullName>
        <active>true</active>
        <description>If free selection is set, this rule forces input of number and period</description>
        <errorConditionFormula>AND( 
 ISPICKVAL(Recurrence_Int__c,&quot;Free Selection&quot;),
OR(
ISNULL( Recurrence_Number__c ),
ISBLANK(TEXT( Recurrence_Unit__c) )
)
)</errorConditionFormula>
        <errorDisplayField>Recurrence_Number__c</errorDisplayField>
        <errorMessage>With free selection you have to give number and unit!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RecurrenceIntervalHasToBeSet</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 IsRecurrence__c ,
ISBLANK(TEXT( Recurrence_Int__c ))
)</errorConditionFormula>
        <errorMessage>If recurrence is selected you have to choose an interval!</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>GetData2</fullName>
