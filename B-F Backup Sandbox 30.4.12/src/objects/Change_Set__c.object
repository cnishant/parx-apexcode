    <deploymentStatus>Deployed</deploymentStatus>
    <description>A Change Request is a collection of objects moving to production on a given release day</description>
    <enableActivities>true</enableActivities>
    <enableFeeds>true</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Admin_Access_Required__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Check this box if you need System Administrator access to finish the deployment of this Change Set. e.g. - Manual Approval Process, Profile Changes, etc ...</inlineHelpText>
        <label>Admin Access Required</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Application_Work_Package__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Application Work Package</label>
        <referenceTo>Work_Package__c</referenceTo>
        <relationshipName>Change_Sets</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Biz_Process_Process_Hours__c</fullName>
        <description>This field captures the additional LOE necessary for business process work.</description>
        <externalId>false</externalId>
        <label>Biz Process Design Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Business_Approver__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Approver</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Change_Sets</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Change_Request_Name__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the SalesForce Change Set Name</inlineHelpText>
        <label>SF Change Set Name</label>
        <length>32000</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Configuration_Hours__c</fullName>
        <description>This field captures the additional LOE necessary for configuration.</description>
        <externalId>false</externalId>
        <label>Configuration Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Date_Expected_On__c</fullName>
        <externalId>false</externalId>
        <label>Date Expected On</label>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Deployed__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Deployed?</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Development_Hours__c</fullName>
        <description>This field captures the additional LOE necessary for development.</description>
        <externalId>false</externalId>
        <label>Development Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Infrastructure__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Infrastructure</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Release_Notes__c</fullName>
        <externalId>false</externalId>
        <label>Release Notes</label>
        <length>32000</length>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Releases__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Release</label>
        <referenceTo>Release__c</referenceTo>
        <relationshipLabel>Change Requests</relationshipLabel>
        <relationshipName>Change_Requests</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Requirements_Hours__c</fullName>
        <description>This field captures the additional LOE necessary for information gathering.</description>
        <externalId>false</externalId>
        <label>Requirements Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>Submitted for approval</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Approved</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Denied</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>In Progress</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>New</fullName>
                <default>true</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Summary__c</fullName>
        <externalId>false</externalId>
        <label>Summary</label>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Tech_Design_Hours__c</fullName>
        <description>This field captures the additional LOE necessary for technical design.</description>
        <externalId>false</externalId>
        <label>Tech Design Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Level_of_Effort__c</fullName>
        <description>This field sums up all LOE into the total number of hours for this change request.</description>
        <externalId>false</externalId>
        <formula>Requirements_Hours__c + Biz_Process_Process_Hours__c + Tech_Design_Hours__c + Configuration_Hours__c + Development_Hours__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Level of Effort</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Type__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Application_Work_Package__r.Type__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Change Request</label>
    <listViews>
        <fullName>AllChangeRequests</fullName>
        <columns>NAME</columns>
        <columns>Summary__c</columns>
        <columns>Status__c</columns>
        <columns>Type__c</columns>
        <columns>Releases__c</columns>
        <columns>Admin_Access_Required__c</columns>
        <columns>OWNER.ALIAS</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <label>All Change Sets</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>Change_Requests_Approved</fullName>
        <columns>NAME</columns>
        <columns>Summary__c</columns>
        <columns>Status__c</columns>
        <columns>Total_Level_of_Effort__c</columns>
        <columns>OWNER.ALIAS</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </filters>
        <label>Change Requests Approved</label>
    </listViews>
    <listViews>
        <fullName>Change_Requests_Denied</fullName>
        <columns>NAME</columns>
        <columns>Summary__c</columns>
        <columns>Status__c</columns>
        <columns>Total_Level_of_Effort__c</columns>
        <columns>OWNER.ALIAS</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Denied</value>
        </filters>
        <label>Change Requests Denied</label>
    </listViews>
    <listViews>
        <fullName>My_Change_Requests</fullName>
        <columns>NAME</columns>
        <columns>Summary__c</columns>
        <columns>Status__c</columns>
        <columns>Total_Level_of_Effort__c</columns>
        <columns>OWNER.ALIAS</columns>
        <filterScope>Mine</filterScope>
        <label>My Change Requests</label>
    </listViews>
    <nameField>
        <displayFormat>CR-{0}</displayFormat>
        <label>Change Set ID</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Change Requests</pluralLabel>
