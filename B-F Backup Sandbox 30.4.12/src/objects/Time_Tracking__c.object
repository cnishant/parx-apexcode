        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Staff_Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>PARX - Jochen Schrader: P00002 Account that is related to Staff Member</description>
        <externalId>false</externalId>
        <inlineHelpText>Account that is related to Staff Member</inlineHelpText>
        <label>Staff Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Time Tracking</relationshipLabel>
        <relationshipName>Time_Tracking</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>StartDate_Month__c</fullName>
        <description>PARX - Jochen Schrader: needed to select the time tracking records by outlet visit trigger</description>
        <externalId>false</externalId>
        <formula>Month(StartDate__c)+(Year(StartDate__c)*100)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>StartDate Month</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>StartDate__c</fullName>
        <defaultValue>Date(Year(Today()),Month(Today()),1 )</defaultValue>
        <description>PARX - Jochen Schrader: P00002 The Start Date you are working for B-F this month.</description>
        <externalId>false</externalId>
        <inlineHelpText>The Start Date you are working for B-F this month.</inlineHelpText>
        <label>StartDate</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Startdate_Year__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Year(StartDate__c))</formula>
        <label>Startdate Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
