        <defaultValue>0</defaultValue>
        <description>PARX - Jochen Schrader: P00002 Event days in the selected Month</description>
        <externalId>false</externalId>
        <inlineHelpText>Event days in the selected Month</inlineHelpText>
        <label>Event</label>
        <precision>4</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Illness__c</fullName>
        <defaultValue>0</defaultValue>
        <description>PARX - Jochen Schrader: P00002 - Number of Illness Days in the selected Month</description>
        <externalId>false</externalId>
        <inlineHelpText>Number of Illness Days in the selected Month</inlineHelpText>
        <label>Illness</label>
        <precision>4</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Meeting__c</fullName>
        <defaultValue>0</defaultValue>
        <description>PARX - Jochen Schrader: P00002 Meeting Days in the selected Month</description>
