                        <c:contentblock name="rghtcol2" />
                    </div>
                    <div class="oneColBtm">
                        <c:contentblock name="rghtcol2Btm" />
                    </div>
                </div>
                <!--End oneColInfo -->
            </div>
            <!--End RightSide Content --> 
        <!-- end prodOver -->
    </apex:define>
</apex:composition>
