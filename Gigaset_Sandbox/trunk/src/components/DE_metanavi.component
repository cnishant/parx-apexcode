<apex:component id="metanavi" controller="DETemplateController">
<apex:attribute name="isPro" description="pro or not pro" 

        type="Boolean" required="false" assignTo="{!isPro}"/>
<apex:attribute name="portal" description="portal" 

        type="String" required="false" assignTo="{!portalAttribute}"/>
<ul id="metanavi">
	<li>
		<apex:outputLink value="{!$Site.CustomWebAddress}{!IF(portal!='','?portal='&portal,'')}">{!label_menu_home}</apex:outputLink>
	</li>
	<li>
		<apex:outputLink value="{!ImprintUrl}{!IF(portal!='','&portal='&portal,'')}">{!$Label.tplt_label_imprint}</apex:outputLink>
	</li>
	<apex:outputPanel id="kontaktlink" rendered="{!isAuthenticated}">
	<li>
		<apex:outputLink value="{!KontaktformularUrl}" id="linkcontact" styleClass="{!IF($CurrentPage.parameters.pageid=kontaktpageid,'active','')}" >{!$Label.contact}</apex:outputLink>
	</li>
	</apex:outputPanel>
</ul>
</apex:component>