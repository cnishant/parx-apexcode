<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
    <script type="text/javascript" src="/js/functions.js"></script>
    <script src="/soap/ajax/14.0/connection.js" type="text/javascript"></script>
	<script src="/soap/ajax/12.0/apex.js" type="text/javascript"></script>
	<script>
		function createSeminare() {
			try{
				var message = sforce.apex.execute("PortfolioSeminarHelper","createSeminar",{portfolioId:"{!Portfolio__c.Id}"});
				if(message.length > 0){
					alert(message);
				}
				
				window.parent.location.href = "{!URLFOR($Action.Portfolio__c.View, Portfolio__c.Id)}";
			}catch(e){
				alert('An error occured: '+ e);
				window.parent.location.href = "{!URLFOR($Action.Portfolio__c.View, Portfolio__c.Id)}";
			}
		}
	</script>
</head>
<body onload=createSeminare()>
</div>
</body>
</html>