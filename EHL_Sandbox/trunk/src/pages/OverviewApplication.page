                        </tr>
                        <tr>
                            <td class="labelCol">{!$ObjectType.Education__c.fields.Date_of_Test__c.label}</td>
                            <td class="dataCol">
                                <span>{!DAY(a.Date_of_Test__c)}.{!MONTH(a.Date_of_Test__c)}.{!YEAR(a.Date_of_Test__c)}</span>
                            </td>
                        </tr>
