            <apex:column value="{!a.Date_From__c}"/>
            <apex:column value="{!a.Date_Until__c}"/>
            <apex:column value="{!a.Diploma_Date__c}"/>         
            <apex:column value="{!a.Main_Language_of_Instruction__c}"/>
