<apex:page showHeader="false" standardStylesheets="true" sidebar="false" controller="OverviewApplicationController" action="{!init}" tabStyle="Application__c" >
<apex:pageBlock title="Application Overview">
    <apex:form >
            <apex:commandButton action="{!doNothing}" value="print" onclick="javascript:print()"/>  
    </apex:form>
    <apex:pageBlockSection title="Application Detail">
        <apex:outputField value="{!oApplication.name}"/>
        <apex:outputField value="{!oApplication.OwnerId}"/>
        <apex:outputField value="{!oProduct.Name}"/>
        <apex:outputField value="{!oApplication.Application_Phase__c}"/>
        <apex:outputField value="{!oProduct.Product_Language__c}"/>
        <apex:outputField value="{!oApplication.Application_Status__c}"/>
        <apex:outputField value="{!oProduct.Product_Period__c}"/>
        <apex:outputField value="{!oApplication.Application_Start_Date__c}"/>
        <apex:outputField value="{!oOpportunity.name}"/>
        <apex:outputField value="{!oApplication.Application_Submit_Date__c}"/>
    </apex:pageBlockSection>
    <apex:pageBlockSection title="Candidat">
        <apex:outputField value="{!oContact.LastName}"/>
        <apex:outputField value="{!oApplication.Age_at_Start_of_Programme__c}"/>
        <apex:outputField value="{!oContact.FirstName}"/>
        <apex:outputField value="{!oContact.Nationality_1__c}"/>
        <apex:outputField value="{!oContact.Student_Name__c}"/>
        <apex:outputField value="{!oContact.Nationality_2__c}"/>
        <apex:outputField value="{!oContact.Gender__c}"/>
        <apex:outputField value="{!oContact.Nationality_3__c}"/>
        <apex:outputField value="{!oContact.Birthdate}"/>
        <apex:outputField value="{!oContact.Nationality_4__c}"/>
        <apex:outputField value="{!oContact.Country_of_Birth__c}"/>
        <apex:outputField value="{!oContact.Swiss_Permit__c}"/>
        <apex:outputField value="{!oAddress.Country__c}"/> 
        <apex:outputField value="{!oContact.Date_of_Expiry__c}"/>
        <apex:outputField value="{!oContact.Email_Private__c}"/>
        <apex:outputField value="{!oApplication.Visa_Required__c}"/>
    </apex:pageBlockSection>
    <apex:pageBlockSection title="Language Qualifications">
        <apex:outputLabel styleClass="labelCol" value="English" />
        <label class="labelCol">Français</label>
        <tr>
            <td colspan="2">
                <table>
                    <apex:repeat var="a" value="{!listEducationLQ_EN}">
