.arrows
{
	background-image: url(/img/paginationArrows.gif);
	background-repeat: no-repeat;
	width: 9px;
	height: 10px;
	margin-right:2px;
	margin-left:2px;
}
.headLabel
{
	padding-left:5px;
}
.list-main-div
{
	width:100%;
	max-width:975px;
	min-width:863px;
}
.list-header
{
	position: relative;
	text-align: left;
	padding-left: 450px;
	vertical-align:middle;
}
.list-header-left
{
	position: absolute;
	top:0px;
	left:5px;
}
.list-header-right
{
	position: absolute;
	right:5px;
	top:0px;
}
.list-header-middle
{
   height:20px;
   vertical-align:middle;
   padding-top:5px;
}
.detail-main-div
{
	width:100%;
	max-width:975px;
	min-width:375px;
}
.detail-padding
{
	padding-top:10px;
}
.edit-field-td
{
	padding:10px;
	margin:5px;
	vertical-align:middle;
}
.memonic_title a 
{
	color: #1c5cb1;
}
.collapsable a, .source a,.note-expanded  .date a 
{
	color: #666;
	font-size: 11px;
	text-decoration:none;
}
.cbox a
{
	margin-bottom: 5px;
	color: #036;
	display: inline;
	font-weight: bold;
}

.collapsable {cursor: pointer;}
.cbox 
{
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	padding: 0.769em 0.769em 0.669em 1.462em;
	border-radius: 6px;
	box-shadow: #444 2px 2px 2px;
	-moz-box-shadow: #444 2px 2px 2px;
	-webkit-box-shadow: #444 2px 2px 2px;
	background: #fff;
	margin: 8px 0px;
	position: relative;
	border: 1px solid transparent;
}

.abstract {overflow: hidden; white-space: nowrap; text-overflow: ellipsis-word;}
.absMargin
{
	margin:5px 0px;
	padding-bottom:0px
}
.memonic-metadata
{
	float: right;
	padding-left: 15px;
	white-space: nowrap;
	vertical-align:top;
	margin: 1px 0px 5px 0px;
}
.noteH2 
{
	float: left;
	font-size: 1em;
	margin-right: 0.385em;
	max-width: 50em;
	overflow: hidden;
	white-space: nowrap;
}
.cbox-sum
{
	
}

.shortDate
{
	margin-bottom:10px;
}
.cbox-list
{
	padding: 0.2em 0.769em 0em 1.462em;
	margin:5px 0px;
}

.cbox h2 {color: #036; display: inline; font-weight: bold;}
.note-expanded .date {color: #666; font-size: 11px;}
.note-expanded h2, .note-expanded {margin-bottom: 5px;}
.note .owner, .note-expanded .source {border-left: 1px solid #ccc; padding-left: 5px; padding-right: 3px;}
.note-expanded .source {padding-left: 6px;}
.note-expanded h2 {font-size: 1.24em;}
.note-detail h2, .note-detail, .note-detail .body, .note-detail .organize {margin-bottom: 5px;}
.toolbar_fixed .note_toolbar {-webkit-border-radius: 0; -moz-border-radius: 0; border-radius: 0;}

.box 
{
	box-shadow: #444 2px 2px 2px;
	-moz-box-shadow: #444 2px 2px 2px;
	-webkit-box-shadow: #444 2px 2px 2px;
	-webkit-border-radius: 0.462em;
	-moz-border-radius: 0.462em;
	border-radius: 0.462em;
	background: #fff;
	margin-bottom: 20px;
	padding: 15px 20px 20px 20px;
}
.active {border-color: #bbb;}
.box h2
{
	font-size: 36px;
	font-weight: normal;
	margin-bottom: 15px;
	text-align: center;
}
.bookmarklet-button 
{
	color: #fff !important;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	border-radius: 6px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;
	padding: 10px 15px 10px 50px;
	text-decoration: none;
	display: inline-block;
	float: none; margin: 12px;
	background: #004c98 url(http://s.memonic.ch/img/boxes/bookmarklet_squirrel.png) no-repeat 10px 7px;
	background: url(http://s.memonic.ch/common/0.1.dev-r32018/gz/img/boxes/bookmarklet_squirrel.png) no-repeat 10px 7px, -webkit-gradient(linear, left top, left bottom, from(#004c98), to(#003366));
	background: url(http://s.memonic.ch/common/0.1.dev-r32018/gz/img/boxes/bookmarklet_squirrel.png) no-repeat 10px 7px, -moz-linear-gradient(top, #004c98, #003366);
}

.hideable
{
	overflow: hidden;
	white-space: nowrap;
}


