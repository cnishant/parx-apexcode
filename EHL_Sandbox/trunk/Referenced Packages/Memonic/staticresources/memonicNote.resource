.nektoon_body h1 {font-size: 2em; margin-bottom: 0.5em;}
.nektoon_body h2 {font-size: 1.4em; margin-bottom: 0.5em;}
.nektoon_body h3 {font-size: 1.1em; margin-bottom: 1.1em;}
.nektoon_body p, .nektoon_body pre, .nektoon_body ul, .nektoon_body ol {margin-bottom: 1em;}
.nektoon_body table {border-left: 1px solid #ddddda; margin: 0.5em 0 1em 0;}
.nektoon_body table th {border: 1px solid #ddddda; padding: 0.3em 0.6em;}
.nektoon_body table th {background: #e9e9e6; letter-spacing: 1px; text-transform: uppercase; font-size: 0.9em;}
.nektoon_body li {margin-left: 2em;}
